double n1 = 0;
double n2 = 0;
Console.WriteLine("Digite o n1:");
n1 = Convert.ToDouble(Console.ReadLine());
Console.WriteLine("Digite o n2:");
n2 = Convert.ToDouble(Console.ReadLine());
Console.WriteLine($"Os números digitados são,{n1} e {n2}");
if (n1 == n2)
{
    Console.WriteLine($"Os Nímeros são iguais");
}
else if (n1 > n2)
{
    Console.WriteLine($"O Número {n1} é maior que {n2}");
}
else
{
    Console.WriteLine($"O Número {n2} é maior que {n1}");
